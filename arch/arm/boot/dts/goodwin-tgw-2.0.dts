/dts-v1/;
/include/ "goodwin-gw-9x25.dtsi"

/ {
	model = "Goodwin TGW 2.0";
	compatible = "goodwin,tgw-2.0", "atmel,at91sam9x5", "atmel,at91sam9";

	ahb {
		apb {
			usart2: serial@f8024000 {
				status = "okay";
			};

			usart3: serial@f8028000 {
				status = "okay";
			};
		};
	};
};

